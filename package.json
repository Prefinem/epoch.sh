{
	"author": "Prefinem",
	"dependencies": {
		"lambdify": "4.3.1",
		"mustache": "4.0.1"
	},
	"description": "Epoch.sh Lambda Functions",
	"devDependencies": {
		"eslint": "7.7.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-jest": "23.20.0",
		"eslint-plugin-react": "7.20.6",
		"husky": "4.2.5",
		"jest": "26.4.2",
		"lint-staged": "10.2.13",
		"prettier": "2.1.1",
		"serverless": "1.79.0",
		"serverless-apigw-binary": "0.4.4",
		"serverless-content-encoding": "1.1.0",
		"serverless-domain-manager": "4.2.0",
		"serverless-offline": "6.6.0",
		"serverless-plugin-log-retention": "2.0.0",
		"serverless-prune-plugin": "1.4.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn staged"
		}
	},
	"license": "MIT",
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --config ./.prettierrc.json --write",
			"jest --bail --findRelatedTests",
			"git add"
		]
	},
	"name": "epoch.sh",
	"repository": "https://github.com/Prefinem/epoch.sh",
	"scripts": {
		"clean": "yarn lint && yarn prettify",
		"deploy": "sls deploy",
		"eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
		"lint": "eslint src/* --fix",
		"prettify": "prettier --config ./.prettierrc.json --write \"src/**\"",
		"staged": "lint-staged",
		"start": "sls offline start"
	},
	"version": "2.0.0"
}
